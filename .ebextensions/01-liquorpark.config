packages:
  yum:
    postgresql94-devel: []

container_commands:
  01_setup_apache:
    command: "cp .ebextensions/enable_mod_*.conf /etc/httpd/conf.d"

option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "project.settings"
    PYTHONPATH: "/opt/python/current/app/liquorpark:$PYTHONPATH"
    SECRET_KEY: "abc"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: "project/wsgi.py"
  "aws:elasticbeanstalk:container:python:staticfiles":
    "/static/": "staticfiles/"
